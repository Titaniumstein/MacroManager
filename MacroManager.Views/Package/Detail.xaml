<UserControl x:Class="MacroManager.Views.Package.Detail"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MacroManager.Views.Package"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel LastChildFill="False">
        <Label DockPanel.Dock="Top">Detail</Label>
        <local:ReadPackage x:Name="packageControl" DockPanel.Dock="Top" />

    </DockPanel>
</UserControl>