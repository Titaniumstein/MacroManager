<UserControl x:Class="MacroManager.Views.Package.Delete"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MacroManager.Views.Package"
             xmlns:controls="clr-namespace:MacroManager.Views.Controls"

             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
    <StackPanel>
        <controls:PageTitle Content="Delete"/>
        <controls:PageContent>
            <StackPanel>
                <local:ReadPackage x:Name="packageControl"/>
                <controls:MyButton 
                x:Name="delete"
                Click="Delete_Click"
                HorizontalAlignment="Left"
                Content="Delete"
                />
            </StackPanel>
        </controls:PageContent>
       


    </StackPanel>
</UserControl>